{
  "name": "antec-terminal-portfolio",
  "version": "1.0.0",
  "description": "ANTEC Terminal Portfolio - Enterprise Monorepo with Authentication",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev:web": "npm run dev --workspace=apps/web",
    "dev:backend": "npm run dev --workspace=apps/backend",
    "dev:all": "concurrently \"npm run dev:web\" \"npm run dev:backend\"",
    "install:all": "npm install && npm run install --workspaces"
  },
  "devDependencies": {
    "turbo": "^2.3.3",
    "concurrently": "^9.1.0"
  },
  "packageManager": "npm@10.9.4",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/antik1108/Terminal-Portfolio-ANTEC"
  },
  "author": "Antik Mondal",
  "license": "MIT"
}